<!DOCTYPE html>
<html>

<!-- Head -->
<head>

    <title>@InformationHub.Resource.Resource.Web_Title</title>
    <link rel="shortcut icon" href="~/img/favicon.ico" type="image/x-icon" />
    <!-- Style -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/login.css" rel="stylesheet" />

    <!-- Fonts -->
    <link href="//fonts.googleapis.com/css?family=Quicksand:300,400,500,700" rel="stylesheet">
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <link href="~/Content/toastr/toastr.min.css" rel="stylesheet" />
    <script src="~/Content/toastr/toastr.js"></script>
    <script src="~/Scripts/ntscommon.js"></script>
    <script src="~/ScriptControllers/LoginController.js"></script>
    <script src="~/ScriptControllers/Setting/AppSetting.js"></script>
    <!-- //Fonts -->

</head>
<!-- //Head -->
<!-- Body -->
<body style="position:relative">
   
    <div class="left">
        <div id="loader_id"></div>
        <div id="overlay" onclick="off()"></div>
        <h1>
            <img class="logo" src="/img/InfomationHub.png" />
        </h1>
        @RenderBody()
        <div class="footer">
            <script>
                document.write("<p> &copy;" + new Date().getFullYear() + " Case Management</p>");
            </script>
        </div>

    </div>
    @*<div class="right">

    </div>*@
</body>
<!-- //Body -->

</html>