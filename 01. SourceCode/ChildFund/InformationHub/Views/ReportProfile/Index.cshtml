@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    string type = ViewBag.Type;
    var dateNow = DateTime.Now;
    //var dateFrom = "01/01/" + dateNow.Year;
    var dateFrom = "";
    var dateTo = dateNow.ToString("dd/MM/yyyy");
}
<section class="wrapper main-wrapper">
    <div ui-view="" class="ng-scope">
        <div class="content-wrapper">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <section class="box collapsed">
                    <header class="panel_header">
                        <h2 class="title pull-left">@InformationHub.Resource.Resource.Search_Title</h2>
                        <div class="actions panel_actions pull-right">
                            <i class="box_toggle material-icons">expand_more</i>
                        </div>
                    </header>
                    <div class="content-body padding-bottom-10">
                        <div class="row">
                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.ReportProfile_ChildName/@InformationHub.Resource.Resource.Code_Report</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <input type="text" class="form-control" id="ChildName">
                                        <input type="hidden" value="@type" id="type_User" />
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.ReportProfile_Gender</label>
                                    <div class="controls">
                                        <select class="form-control" id="Gender">
                                            <option value="">@InformationHub.Resource.Resource.All_Title</option>
                                            <option value="0">@InformationHub.Resource.Resource.Unknow_Gender</option>
                                            <option value="1">@InformationHub.Resource.Resource.ReportProfile_Male</option>
                                            <option value="2">@InformationHub.Resource.Resource.ReportProfile_FeMale</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.Label_Age</label>
                                    <div class="controls">
                                        <select class="form-control" id="Age">
                                            <option value="">@InformationHub.Resource.Resource.All_Title</option>
                                            <option value="-1;4">@InformationHub.Resource.Resource.Statistic_Age0_3</option>
                                            <option value="3;7">@InformationHub.Resource.Resource.Statistic_Age4_6</option>
                                            <option value="6;10">@InformationHub.Resource.Resource.Statistic_Age7_9</option>
                                            <option value="9;11">@InformationHub.Resource.Resource.Statistic_Age10</option>
                                            <option value="10;13">@InformationHub.Resource.Resource.Statistic_Age11_12</option>
                                            <option value="12;15">@InformationHub.Resource.Resource.Statistic_Age13_14</option>
                                            <option value="14;17">@InformationHub.Resource.Resource.Statistic_Age15_16</option>
                                            <option value="16;18">@InformationHub.Resource.Resource.Statistic_Age16_18</option>
                                            <option value="17;100">@InformationHub.Resource.Resource.Statistic_Age18</option>
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.ReportProfile_InformationSources</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <select class="form-control" id="InformationSources">
                                            <option value="">@InformationHub.Resource.Resource.All_Title</option>
                                            <option value="0">@InformationHub.Resource.Resource.Phone_Title</option>
                                            <option value="1">@InformationHub.Resource.Resource.Live_Title</option>
                                            <option value="2">@InformationHub.Resource.Resource.Other_Title</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.ReportProfile_AbuseType</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <select class="form-control" id="AbuseId">
                                            <option value="">@InformationHub.Resource.Resource.All_Title</option>
                                            @Html.Action("GetAllAbuseType", "Combobox", new { type = "0" })
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.ReportProfile_ReportBy</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <input type="text" class="form-control" id="ProviderName">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.ReportProfile_Status</label>
                                    <div class="controls">
                                        <select class="form-control" id="ProcessingStatus">
                                            <option value="">@InformationHub.Resource.Resource.All_Title</option>
                                            <option value="1">@InformationHub.Resource.Resource.ReportProfile_Status1</option>
                                            <option value="2">@InformationHub.Resource.Resource.ReportProfile_Status2</option>
                                            <option value="3">@InformationHub.Resource.Resource.ReportProfile_Status3</option>
                                            <option value="4">@InformationHub.Resource.Resource.ReportProfile_Status4</option>
                                            <option value="5">@InformationHub.Resource.Resource.ReportProfile_Status5</option>
                                            <option value="6">@InformationHub.Resource.Resource.ReportProfile_Status6</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.ReportProfile_SeverityLevel</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <select class="form-control" id="SeverityLevel">
                                            <option value="">@InformationHub.Resource.Resource.All_Title</option>
                                            <option value="0">@InformationHub.Resource.Resource.ReportProfile_Low</option>
                                            <option value="1">@InformationHub.Resource.Resource.ReportProfile_Nomal</option>
                                            <option value="2">@InformationHub.Resource.Resource.ReportProfile_Hight</option>
                                        </select>
                                    </div>
                                </div>
                            </div>


                            @Html.Action("GetAddressByUser", "Combobox")
                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.ReportProfile_ReportAddress</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <input type="text" class="form-control" id="CaseLocation">
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.DayFrom_Title</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <input type="hidden" value="@dateFrom" id="DateFrom_hid" />
                                        <input type="text" class="form-control datepicker" value="@dateFrom" id="DateFrom">
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-3 col-sm-3 col-xs-3">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">@InformationHub.Resource.Resource.ReportProfile_DateTo</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <input type="hidden" value="@dateTo" id="DateTo_hid" />
                                        <input type="text" class="form-control datepicker" value="@dateTo" id="DateTo">
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-8 col-md-6 col-sm-6 col-xs-6 text-right">
                                <div class="form-group margin-top-20">
                                    <button onclick="Search(0)" class="btn btn-success btn-icon right10">
                                        <i class="fa fa-search"></i> &nbsp; <span>@InformationHub.Resource.Resource.Search_Title</span>
                                    </button>
                                    <button onclick="Refresh()" class="btn btn-default btn-icon">
                                        <i class="fa fa-refresh"></i> &nbsp; <span>@InformationHub.Resource.Resource.Reset_Title</span>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <div class="col-lg-12 col-md-12">
                <section class="box ">
                    <header class="panel_header">
                        <h2 class="title pull-left">@InformationHub.Resource.Resource.ReportProfile_ListView</h2>
                        <div class="actions panel_actions pull-right">
                            <i class="box_toggle material-icons">expand_more</i>
                        </div>
                    </header>
                    <div class="content-body padding-bottom-10">
                        <div class="row margin-bottom-5">
                            <div class="col-lg-8 col-md-7 col-sm-7 col-xs-7 text-left">
                                <h4>
                                    @InformationHub.Resource.Resource.Total_Title <b class="badge badge-primary" id="count_data"></b> @InformationHub.Resource.Resource.Report_Title
                                    (<span class="text-warning">@InformationHub.Resource.Resource.Report_Open: <b id="count_data1"></b></span>
                                    - <span class="text-primary">@InformationHub.Resource.Resource.ReportProfile_Status6: <b id="count_data6"></b></span>)
                                </h4>
                            </div>
                            <div class="col-lg-4 col-md-5 col-sm-5 col-xs-5 text-right">
                                <a href="#" id="linkDowload"></a>
                                @if (type == "3")
                                {
                                    if (InformationHub.Common.LoginCommon.CheckPermission("C0011"))
                                    {
                                        <a href="/ReportProfile/CreateProfileView" class="btn btn-success btn-icon right10">
                                            <i class="fa fa-plus"></i> &nbsp; <span>@InformationHub.Resource.Resource.Add_Title</span>
                                        </a>
                                    }
                                }
                                <button class="btn btn-warning btn-icon" onclick="Search(1)" type="button">
                                    <i class="fa fa-file-excel-o"></i> &nbsp; <span>@InformationHub.Resource.Resource.Export_Title</span>
                                </button>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <div class="margin-bottom-10" id="list_data">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12 margin-bottom-10">
                                <hr class="margin-bottom-10 margin-top-0" />
                                <button class="btn btn-xs btnfuntion" title="@InformationHub.Resource.Resource.ReportProfile_ViewDetail">
                                    <i class="material-icons btnicon" style="color:#1656cb">assignment_ind</i>
                                </button>&nbsp;<small class="padding-right-20">@InformationHub.Resource.Resource.ReportProfile_ViewDetail</small>
                                @if (type == "3")
                                {
                                    <button class="btn btn-xs btnfuntion" title="@InformationHub.Resource.Resource.ReportProfile_Update">
                                        <i class="material-icons btnicon" style="color:orange">edit</i>
                                    </button> <small class="padding-right-20">@InformationHub.Resource.Resource.ReportProfile_Update</small>

                                    <button class="btn btn-xs btnfuntion" title="@InformationHub.Resource.Resource.Publish_Title">
                                        <i class="material-icons btnicon" style="color:forestgreen">publish</i>
                                    </button> <small class="padding-right-20">@InformationHub.Resource.Resource.Publish_Title</small>
                                }
                                @if (type == "0")
                                {
                                    <button class="btn btn-xs btnfuntion" title="@InformationHub.Resource.Resource.Delete_Title">
                                        <i class="material-icons btnicon" style="color:red">delete_sweep</i>
                                    </button> <small class="padding-right-20">@InformationHub.Resource.Resource.Delete_Title</small>

                                    <button class="btn btn-xs btnfuntion" title="@InformationHub.Resource.Resource.ReOpen">
                                        <i class="material-icons btnicon" style="color:green">replay</i>
                                    </button> <small class="padding-right-20">@InformationHub.Resource.Resource.ReOpen</small>
                                }
                            </div>
                        </div>

                    </div>
                </section>
            </div>
        </div>
    </div>
</section>

<div id="modamPublish" class="modal fade" role="dialog" onscroll="scroll" data-backdrop="static" style="background-color: rgba(0,0,0,0.3)">
    <div class="modal-dialog" style="width:500px">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="h4Title">@InformationHub.Resource.Resource.Warning_Title</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <label id="labelPublish"></label>
                            <input type="hidden" id="valuePublish" />
                        </div>
                    </div>
                </div>
                <hr />
                <div class="modal-footer">
                    <button type="button" onclick="Publish()" class="btn btn-primary" id="btn-createContinue"><i class="glyphicon glyphicon-floppy-save"></i> @InformationHub.Resource.Resource.Yes_Title</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="glyphicon glyphicon-minus-sign"></i> @InformationHub.Resource.Resource.Exit_Title</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="modalDeleteCase" class="modal fade" role="dialog" onscroll="scroll" data-backdrop="static" style="background-color: rgba(0,0,0,0.3)">
    <div class="modal-dialog" style="width:500px">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="h4Title">@InformationHub.Resource.Resource.Warning_Title</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <label id="labelDeleteCase"></label>
                            <input type="hidden" id="valueDeleteCase" />
                        </div>
                    </div>
                </div>
                <br />
                <div class="modal-footer">
                    <button type="button" onclick="DeleteCase()" class="btn btn-primary" id="btn-createContinue"><i class="glyphicon glyphicon-floppy-save"></i> @InformationHub.Resource.Resource.Yes_Title</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="glyphicon glyphicon-minus-sign"></i> @InformationHub.Resource.Resource.Exit_Title</button>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="modalReOpen" class="modal fade" role="dialog" onscroll="scroll" data-backdrop="static" style="background-color: rgba(0,0,0,0.3)">
    <div class="modal-dialog" style="width:500px">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="h4Title">@InformationHub.Resource.Resource.Warning_Title</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="form-group">
                            <label id="labelReOpen"></label>
                            <input type="hidden" id="valueReOpen" />
                        </div>
                    </div>
                </div>
                <br />
                <div class="modal-footer">
                    <button type="button" onclick="ReOpen()" class="btn btn-primary" id="btn-createContinue"><i class="glyphicon glyphicon-floppy-save"></i> @InformationHub.Resource.Resource.Yes_Title</button>
                    <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="glyphicon glyphicon-minus-sign"></i> @InformationHub.Resource.Resource.Exit_Title</button>
                </div>
            </div>
        </div>
    </div>
</div>


<div id="modalForwardReport" class="modal fade" role="dialog" tabindex="-1" onscroll="scroll" data-backdrop="static" style="background-color: rgba(0,0,0,0.3)">
    <div class="modal-dialog modal-sm" style="width:700px">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="h4Title">@InformationHub.Resource.Resource.NoteForward_Title</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12 no-padding">
                        <div class="col-sm-12">
                            <div class="form-group">
                                <span class="input-icon icon-right">
                                    <label>@InformationHub.Resource.Resource.NoteForward_Title</label>&nbsp;<span class="required">*</span>
                                    <textarea type="text" class="form-control" rows="8" id="ForwardNote"></textarea>
                                    <input type="hidden" id="ReportProfileId" />
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" onclick="SendForward()" class="btn btn-primary" id="btn-reset"><i class="glyphicon glyphicon-floppy-save"></i> @InformationHub.Resource.Resource.Save_Title</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal"><i class="glyphicon glyphicon-minus-sign"></i> @InformationHub.Resource.Resource.Exit_Title</button>
            </div>
        </div>
    </div>
</div>
<script src="~/ScriptControllers/ReportProfileListControllers.js"></script>
<script>
    window.onload = Search(0);
</script>

