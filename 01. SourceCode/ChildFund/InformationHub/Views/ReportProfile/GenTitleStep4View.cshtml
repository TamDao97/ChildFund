@model IList<InformationHub.Model.ComboboxResult>
@{int rowIndex = 0;}
@if (Model.Count > 1)
{
    <section class="box ">
        <header class="panel_header">
            <h2 class="title pull-left">@InformationHub.Resource.Resource.List_Plant</h2>
            <div class="actions panel_actions pull-right">
                <i class="box_toggle material-icons">expand_more</i>
            </div>
        </header>
        <div class="content-body padding-bottom-5 margin-bottom-10">
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <table class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th width="70px" class="text-center">@InformationHub.Resource.Resource.Index_Title</th>
                                <th width="100px" class="text-center">@InformationHub.Resource.Resource.Funtion_Title</th>
                                <th width="160px">@InformationHub.Resource.Resource.Label_Planning_Date</th>
                                <th>@InformationHub.Resource.Resource.Plant_Title</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                rowIndex++;
                            <tr>
                                <td class="text-center">@rowIndex</td>
                                <td class="text-center">
                                    <button onclick="GenStep4View('@item.Id')" type="button" class="btn btn-xs btnfuntion" title="@InformationHub.Resource.Resource.Label_Update"><i class="material-icons btnicon" style="color:orange">edit</i></button>
                                    <button onclick="DeleteSupportPlant('@item.Id')" type="button" class="btn btn-xs btnfuntion" title="@InformationHub.Resource.Resource.Delete_Title"><i class="material-icons btnicon" style="color:red">close</i></button>
                                </td>
                                <td>@item.Exten</td>
                                <td>@item.Name</td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </section>
    <div class="clearfix"></div>
}

