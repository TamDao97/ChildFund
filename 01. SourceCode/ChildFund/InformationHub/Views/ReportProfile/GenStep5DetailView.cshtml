    $("input[name=SupAfAbilityKnowGuard][value=" + @Model.AbilityKnowGuard.ToString() + "]").prop('checked', true);
    $("input[name=SupAfAbilityHelpOthers][value=" + @Model.AbilityHelpOthers.ToString() + "]").prop('checked', true);

    $('#table1_tab4 input[type="radio"]').on('change', function (e) {
        CounTable1Tab4();
        AutoCheckTab4();
    });
    $('#table2_tab4 input[type="radio"]').on('change', function (e) {
