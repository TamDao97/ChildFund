<script type="text/javascript">
    $('.datepickerfml2').datepicker({
        format: 'dd/mm/yyyy',
        endDate: '+0d',
        autoclose: true
    });
     //set giá trị step 4
    $("input[name=SupAfLevelHarm][value=" + @Model.LevelHarm.ToString() + "]").prop('checked', true);
    $("input[name=SupAfLevelApproach][value=" + @Model.LevelApproach.ToString() + "]").prop('checked', true);
    $("input[name=SupAfLevelCareObstacle][value=" + @Model.LevelCareObstacle.ToString() + "]").prop('checked', true);

