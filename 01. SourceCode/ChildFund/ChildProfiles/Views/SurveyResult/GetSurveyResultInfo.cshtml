@model ChildProfiles.Model.SurveyResult.SurveyResultModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="wrapper main-wrapper">
    <div ui-view="" class="ng-scope">
        <div class="content-wrapper">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <section class="box ">
                    <header class="panel_header">
                        <h2 class="title pull-left" id="h2title">Chi tiết kết quả khảo sát</h2>
                        <div class="actions panel_actions pull-right">
                            <i class="box_toggle material-icons">expand_more</i>
                        </div>
                    </header>
                    <div class="content-body padding-bottom-10">
                        <form id="sFrom">
                            <div class="row">
                                <div class="col-md-12">
                                    <h4 class="margin-bottom-5 bold text-success">Thông tin khảo sát</h4>
                                    <hr />
                                </div>
                                <div class="col-md-12 col-sm-12 col-xs-12 no-padding">
                                    <div class="col-md-4 col-sm-4 col-xs-4">
                                        <div class="form-group">
                                            <label class="form-label">Tên khảo sát</label>&nbsp;
                                            <div class="controls">
                                                <input disabled class="form-control" maxlength="150" id="nameSurvey" value="@Model.SurveyName">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-4 col-xs-4">
                                        <div class="form-group">
                                            <label class="form-label">Người tạo</label>&nbsp;
                                            <div class="controls">
                                                <input disabled class="form-control" maxlength="150" id="nameUser" value="@Model.UserName">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-4 col-xs-4">
                                        <div class="form-group">
                                            <label class="form-label">Thời gian khởi tạo</label>&nbsp;
                                            <div class="controls">
                                                <input disabled value="@Model.CreateDate.Value.ToString("dd/MM/yyyy")" class="form-control" name="createDate" id="createDate">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <h4 class="margin-bottom-5 bold text-success">Chủ đề và câu hỏi</h4>
                                    <hr />
                                </div>
                                <div class="col-md-11 col-sm-11 col-xs-11 no-padding" id="data-all">
                                    @Html.Action("GenGroupQuestionResult", "SurveyResult", new { ListGroupQuestion = Model.ListGroupQuestion })
                                </div>
                            </div>

                        </form>

                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12 text-right">
                                <div class="form-group margin-bottom-10 ">
                                    <a href="/SurveyResult/Index" class="btn btn-default btn-icon">
                                        <i class="fa fa-power-off"></i> &nbsp; <span>Thoát</span>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>
<script src="~/ScriptControllers/SurveyResultDetailController.js"></script>
<script>
    window.onload = InitModel('@Model.Id');
</script>