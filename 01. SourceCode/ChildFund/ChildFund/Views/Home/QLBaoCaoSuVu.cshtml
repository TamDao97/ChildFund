                                                <th width="120px">Tổng</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <th scope="row">1</th>
                                                <td>Bạo lực</td>
                                                <td>10</td>
                                                <td>5</td>
                                                <td>2</td>
                                                <td>1</td>
                                                <td>18</td>
                                            </tr>
                                            <tr>
                                                <th scope="row">2</th>
                                                <td>Xâm hại tình dục</td>
                                                <td>25</td>
                                                <td>13</td>
                                                <td>1</td>
                                                <td>5</td>
                                                <td>44</td>
                                            </tr>
                                            <tr>
                                                <th scope="row">3</th>
                                                <td>Bỏ đói</td>
                                                <td>2</td>
                                                <td>15</td>
                                                <td>22</td>
                                                <td>10</td>
                                                <td>47</td>
                                            </tr>
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th colspan="2" class="text-center">Tổng</th>
                                                <th>27</th>
                                                <th>33</th>
                                                <th>25</th>
                                                <th>16</th>
                                                <th>109</th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-6 col-xs-6">
                                <div id="baocaoQuanLy" style="min-width: 310px; height: 430px; margin: 0 auto"></div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>


