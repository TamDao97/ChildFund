
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="wrapper main-wrapper">
    <div ui-view="" class="ng-scope">
        <div class="content-wrapper">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <section class="box ">
                    <header class="panel_header">
                        <h2 class="title pull-left">Báo cáo sự vụ theo trạng thái xử lý</h2>
                        <div class="actions panel_actions pull-right">
                            <i class="box_toggle material-icons">expand_more</i>
                        </div>
                    </header>
                    <div class="content-body padding-bottom-0">
                        <div class="row">
                            <div class="col-md-4 col-sm-4 col-xs-4">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">Loại xâm hại</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <select class="form-control ">
                                            <option value="Default Select">Tất cả</option>
                                            <option value="Option 1">Option 1</option>
                                            <option value="Option 2">Option 2</option>
                                            <option value="Option 3">Option 3</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2 col-sm-2 col-xs-2">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">Trạng thái xử lý</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <select class="form-control ">
                                            <option value="Default Select">Tất cả</option>
                                            <option value="Option 1">Option 1</option>
                                            <option value="Option 2">Option 2</option>
                                            <option value="Option 3">Option 3</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2 col-sm-2 col-xs-2">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">Tỉnh/Thành Phố</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <select class="form-control ">
                                            <option value="Default Select">Tất cả</option>
                                            <option value="Option 1">Option 1</option>
                                            <option value="Option 2">Option 2</option>
                                            <option value="Option 3">Option 3</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2 col-sm-2 col-xs-2">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">Quận/Huyện</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <select class="form-control ">
                                            <option value="Default Select">Tất cả</option>
                                            <option value="Option 1">Option 1</option>
                                            <option value="Option 2">Option 2</option>
                                            <option value="Option 3">Option 3</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2 col-sm-2 col-xs-2">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">Xã/Phường</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <select class="form-control ">
                                            <option value="Default Select">Tất cả</option>
                                            <option value="Option 1">Option 1</option>
                                            <option value="Option 2">Option 2</option>
                                            <option value="Option 3">Option 3</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-4 col-xs-4">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">Ngày báo cáo từ ngày</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <input type="text" class="form-control" id="field-1">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4 col-sm-4 col-xs-4">
                                <div class="form-group">
                                    <label class="form-label" for="field-1">Đến ngày</label>
                                    <span class="desc"></span>
                                    <div class="controls">
                                        <input type="text" class="form-control" id="field-1">
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-4 col-sm-4 col-xs-4 text-right">
                                <div class="form-group margin-top-20">
                                    <button class="btn btn-success btn-icon right10">
                                        <i class="fa fa-search"></i> &nbsp; <span>Tìm kiếm</span>
                                    </button>
                                    <button class="btn btn-default btn-icon">
                                        <i class="fa fa-refresh"></i> &nbsp; <span>Làm mới</span>
                                    </button>
                                </div>
                            </div>
                        </div>
                        <hr />
