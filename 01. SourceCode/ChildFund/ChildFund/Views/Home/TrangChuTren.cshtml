
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="wrapper main-wrapper">
    <div ui-view="" class="ng-scope">
        <div class="content-wrapper">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <section class="box ">
                    <header class="panel_header">
                        <h2 class="title pull-left">Báo cáo sự vụ năm 2018</h2>
                        <div class="actions panel_actions pull-right">
                            <i class="box_toggle material-icons">expand_more</i>
                        </div>
                    </header>
                    <div class="content-body padding-bottom-0">
                        <div class="row">
                            <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
                                <div class="tile-counter bg-warning">
                                    <div class="content">
                                        <i class="fa fa-meh-o icon-lg"></i>
                                        <h2 count-to="1001" value="0" duration="5" class="number_counter ng-scope">70</h2>
                                        <div class="clearfix"></div>
                                        <span>Thể chất</span>

                                        <div class="progress ng-isolate-scope margin-top-5" value="40">
                                            <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" aria-valuetext="40%" aria-labelledby="progressbar" style="min-width: 0px; width: 40%;" ng-transclude=""></div>
                                        </div>
                                        <span>Tăng 40% so năm 2017</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
                                <div class="tile-counter bg-info">
                                    <div class="content">
                                        <i class="fa fa-child icon-lg"></i>
                                        <h2 count-to="1001" value="0" duration="5" class="number_counter ng-scope">50</h2>
                                        <div class="clearfix"></div>
                                        <span>Tình dục</span>

                                        <div class="progress ng-isolate-scope margin-top-5" value="40">
                                            <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" aria-valuetext="60%" aria-labelledby="progressbar" style="min-width: 0px; width: 60%;" ng-transclude=""></div>
                                        </div>
                                        <span>Tăng 60% so năm 2017</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
                                <div class="tile-counter bg-secondary">
                                    <div class="content">
                                        <i class="fa fa-user icon-lg"></i>
                                        <h2 count-to="1601" value="100" duration="5" class="number_counter ng-scope">10</h2>
                                        <div class="clearfix"></div>
                                        <span>Tinh thần</span>

                                        <div class="progress ng-isolate-scope margin-top-5" value="40">
                                            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" aria-valuetext="80%" aria-labelledby="progressbar" style="min-width: 0px; width: 80%;" ng-transclude=""></div>
                                        </div>
                                        <span>Giảm 80% so năm 2017</span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
                                <div class="tile-counter bg-accent">
                                    <div class="content">
                                        <i class="fa fa-user icon-lg"></i>
                                        <h2 count-to="1601" value="100" duration="5" class="number_counter ng-scope">10</h2>
                                        <div class="clearfix"></div>
                                        <span>Xao nhãng</span>

                                        <div class="progress ng-isolate-scope margin-top-5" value="40">
                                            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" aria-valuetext="80%" aria-labelledby="progressbar" style="min-width: 0px; width: 80%;" ng-transclude=""></div>
                                        </div>
                                        <span>Giảm 80% so năm 2017</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                <section class="box ">
                    <header class="panel_header">
                        <h2 class="title pull-left">Báo cáo sự vụ ngày hôm nay</h2>
                        <div class="actions panel_actions pull-right">
                            <i class="box_toggle material-icons">expand_more</i>
                        </div>
                    </header>
                    <div class="content-body padding-bottom-0">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div id="containerNow" style="min-width: 310px; height: 430px; margin: 0 auto"></div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                <section class="box ">
                    <header class="panel_header">
                        <h2 class="title pull-left">Báo cáo sự vụ theo trạng thái xử lý năm 2018</h2>
                        <div class="actions panel_actions pull-right">
                            <i class="box_toggle material-icons">expand_more</i>
                        </div>
                    </header>
                    <div class="content-body padding-bottom-0">
                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                <div id="containerXuLy" style="min-width: 310px; height: 430px; margin: 0 auto"></div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>
        </div>
    </div>
</section>
